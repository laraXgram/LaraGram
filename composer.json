{
  "name": "laraxgram/laragram",
  "description": "LaraGram, an advanced framework for Telegram Bot development!",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "telegram",
    "PHP",
    "laragram",
    "laraquest",
    "bot"
  ],
  "authors": [
    {
      "name": "laraXgram",
      "email": "laraxgram@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories",
      "Database\\Seeder\\": "database/seeders"
    }
  },
  "require": {
    "php": "^8.2",
    "laraxgram/laraquest" : "^1",
    "laraxgram/core": "^1"
  },
  "scripts": {
    "post-autoload-dump": [
      "LaraGram\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php laragram package:discover --ansi"
    ],
    "post-create-project-cmd": [
      "@php laragram migrate --graceful --ansi"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "php laragram serve\" \"php laragram queue:listen --tries=1"
    ]
  },
  "extra": {
    "laragram": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
